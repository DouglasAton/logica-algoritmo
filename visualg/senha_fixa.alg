Algoritmo "senha_fixa"


Var

   senha : inteiro

Inicio

   escreval("Acesso ao Sistema")
   escreval()
   escreva("Digite a senha: ")
   leia(senha)
   
   enquanto senha <> 2002 faca
      escreva("Senha Invalida! Tente novamente: ")
      leia(senha)
   fimenquanto

   escreval()
   escreval("Acesso Permitido!")


Fimalgoritmo